   <statements>
      <def name="First Predecessor Year" context="Patient" accessLevel="Public">
         <expression lowClosed="true" highClosed="false" xsi:type="Interval">
            <low xsi:type="Subtract">
               <operand xsi:type="Start">
                  <operand name="Measurement Period" xsi:type="ParameterRef"/>
               </operand>
               <operand value="1" unit="year" xsi:type="Quantity"/>
            </low>
            <high xsi:type="Start">
               <operand name="Measurement Period" xsi:type="ParameterRef"/>
            </high>
         </expression>
      </def>
      <def name="Lookback Interval One More Year" context="Patient" accessLevel="Public">
         <expression lowClosed="true" highClosed="false" xsi:type="Interval">
            <low xsi:type="Subtract">
               <operand xsi:type="Start">
                  <operand name="Measurement Period" xsi:type="ParameterRef"/>
               </operand>
               <operand value="1" unit="year" xsi:type="Quantity"/>
            </low>
            <high xsi:type="End">
               <operand name="Measurement Period" xsi:type="ParameterRef"/>
            </high>
         </expression>
      </def>
      <def name="Patient" context="Patient">
         <expression xsi:type="SingletonFrom">
            <operand dataType="fhir:Patient" xsi:type="Retrieve"/>
         </expression>
      </def>
      <def name="Initial Population" context="Patient" accessLevel="Public">
         <expression xsi:type="GreaterOrEqual">
            <operand precision="Year" xsi:type="CalculateAgeAt">
               <operand path="birthDate.value" xsi:type="Property">
                  <source name="Patient" xsi:type="ExpressionRef"/>
               </operand>
               <operand xsi:type="Start">
                  <operand name="Measurement Period" xsi:type="ParameterRef"/>
               </operand>
            </operand>
            <operand valueType="t:Integer" value="12" xsi:type="Literal"/>
         </expression>
      </def>
      <def name="Bipolar Disorder Reports" context="Patient" accessLevel="Public">
         <expression xsi:type="Union">
            <operand xsi:type="Union">
               <operand dataType="fhir:DiagnosticReport" codeProperty="code" xsi:type="Retrieve">
                  <codes name="Bipolar Disorder" xsi:type="ValueSetRef"/>
               </operand>
               <operand dataType="fhir:DiagnosticReport" codeProperty="code" xsi:type="Retrieve">
                  <codes name="Bipolar Disorder ECDS" xsi:type="ValueSetRef"/>
               </operand>
            </operand>
            <operand dataType="fhir:DiagnosticReport" codeProperty="code" xsi:type="Retrieve">
               <codes name="Other Bipolar Disorder" xsi:type="ValueSetRef"/>
            </operand>
         </expression>
      </def>
      <def name="PeriodToIntervalOfDT" context="Patient" accessLevel="Public" xsi:type="FunctionDef">
         <expression lowClosed="true" highClosed="true" xsi:type="Interval">
            <low path="value" xsi:type="Property">
               <source path="start" xsi:type="Property">
                  <source name="value" xsi:type="OperandRef"/>
               </source>
            </low>
            <high path="value" xsi:type="Property">
               <source path="end" xsi:type="Property">
                  <source name="value" xsi:type="OperandRef"/>
               </source>
            </high>
         </expression>
         <operand name="value">
            <operandTypeSpecifier name="fhir:Period" xsi:type="NamedTypeSpecifier"/>
         </operand>
      </def>
      <def name="Denominator Exclusion" context="Patient" accessLevel="Public">
         <expression xsi:type="Or">
            <operand xsi:type="Exists">
               <operand xsi:type="Query">
                  <source alias="BD">
                     <expression name="Bipolar Disorder Reports" xsi:type="ExpressionRef"/>
                  </source>
                  <where xsi:type="And">
                     <operand xsi:type="In">
                        <operand path="value" xsi:type="Property">
                           <source path="status" scope="BD" xsi:type="Property"/>
                        </operand>
                        <operand xsi:type="List">
                           <element valueType="t:String" value="preliminary" xsi:type="Literal"/>
                           <element valueType="t:String" value="final" xsi:type="Literal"/>
                           <element valueType="t:String" value="amended" xsi:type="Literal"/>
                           <element valueType="t:String" value="corrected" xsi:type="Literal"/>
                           <element valueType="t:String" value="appended" xsi:type="Literal"/>
                        </operand>
                     </operand>
                     <operand xsi:type="Overlaps">
                        <operand name="PeriodToIntervalOfDT" xsi:type="FunctionRef">
                           <operand asType="fhir:Period" xsi:type="As">
                              <operand path="effective" scope="BD" xsi:type="Property"/>
                           </operand>
                        </operand>
                        <operand name="Lookback Interval One More Year" xsi:type="ExpressionRef"/>
                     </operand>
                  </where>
               </operand>
            </operand>
            <operand xsi:type="Exists">
               <operand xsi:type="Query">
                  <source alias="MDD">
                     <expression dataType="fhir:DiagnosticReport" codeProperty="code" xsi:type="Retrieve">
                        <codes name="Major Depression and Dysthymia" xsi:type="ValueSetRef"/>
                     </expression>
                  </source>
                  <where xsi:type="And">
                     <operand xsi:type="In">
                        <operand path="value" xsi:type="Property">
                           <source path="status" scope="MDD" xsi:type="Property"/>
                        </operand>
                        <operand xsi:type="List">
                           <element valueType="t:String" value="preliminary" xsi:type="Literal"/>
                           <element valueType="t:String" value="final" xsi:type="Literal"/>
                           <element valueType="t:String" value="amended" xsi:type="Literal"/>
                           <element valueType="t:String" value="corrected" xsi:type="Literal"/>
                           <element valueType="t:String" value="appended" xsi:type="Literal"/>
                        </operand>
                     </operand>
                     <operand xsi:type="Overlaps">
                        <operand name="PeriodToIntervalOfDT" xsi:type="FunctionRef">
                           <operand asType="fhir:Period" xsi:type="As">
                              <operand path="effective" scope="MDD" xsi:type="Property"/>
                           </operand>
                        </operand>
                        <operand name="First Predecessor Year" xsi:type="ExpressionRef"/>
                     </operand>
                  </where>
               </operand>
            </operand>
         </expression>
      </def>
      <def name="Denominator 1" context="Patient" accessLevel="Public">
         <expression name="Initial Population" xsi:type="ExpressionRef"/>
      </def>
      <def name="Numerator 1" context="Patient" accessLevel="Public">
         <expression xsi:type="And">
            <operand name="Initial Population" xsi:type="ExpressionRef"/>
            <operand xsi:type="Exists">
               <operand xsi:type="Query">
                  <source alias="DepressionScreen">
                     <expression dataType="fhir:Observation" codeProperty="code" xsi:type="Retrieve">
                        <codes name="Depression Screen" xsi:type="ValueSetRef"/>
                     </expression>
                  </source>
                  <where xsi:type="And">
                     <operand xsi:type="In">
                        <operand path="value" xsi:type="Property">
                           <source path="status" scope="DepressionScreen" xsi:type="Property"/>
                        </operand>
                        <operand xsi:type="List">
                           <element valueType="t:String" value="final" xsi:type="Literal"/>
                           <element valueType="t:String" value="amended" xsi:type="Literal"/>
                        </operand>
                     </operand>
                     <operand xsi:type="In">
                        <operand path="value" xsi:type="Property">
                           <source path="effective" scope="DepressionScreen" xsi:type="Property"/>
                        </operand>
                        <operand name="Measurement Period" xsi:type="ParameterRef"/>
                     </operand>
                  </where>
               </operand>
            </operand>
         </expression>
      </def>
      <def name="Positive Depression Screen During Follow-Up Period" context="Patient" accessLevel="Public">
         <expression xsi:type="Query">
            <source alias="PositiveScreen">
               <expression dataType="fhir:Observation" codeProperty="code" xsi:type="Retrieve">
                  <codes name="Positive Depression Screen" xsi:type="ValueSetRef"/>
               </expression>
            </source>
            <where xsi:type="And">
               <operand xsi:type="In">
                  <operand path="value" xsi:type="Property">
                     <source path="status" scope="PositiveScreen" xsi:type="Property"/>
                  </operand>
                  <operand xsi:type="List">
                     <element valueType="t:String" value="final" xsi:type="Literal"/>
                     <element valueType="t:String" value="amended" xsi:type="Literal"/>
                  </operand>
               </operand>
               <operand xsi:type="In">
                  <operand path="value" xsi:type="Property">
                     <source path="effective" scope="PositiveScreen" xsi:type="Property"/>
                  </operand>
                  <operand name="Measurement Period" xsi:type="ParameterRef"/>
               </operand>
            </where>
         </expression>
      </def>
      <def name="Denominator 2" context="Patient" accessLevel="Public">
         <expression xsi:type="And">
            <operand name="Initial Population" xsi:type="ExpressionRef"/>
            <operand xsi:type="Exists">
               <operand name="Positive Depression Screen During Follow-Up Period" xsi:type="ExpressionRef"/>
            </operand>
         </expression>
      </def>
      <def name="First Positive Depression Screen During Follow-Up Period" context="Patient" accessLevel="Public">
         <expression xsi:type="List">
            <element xsi:type="First">
               <source xsi:type="Query">
                  <source alias="Obs">
                     <expression name="Positive Depression Screen During Follow-Up Period" xsi:type="ExpressionRef"/>
                  </source>
                  <sort>
                     <by direction="asc" xsi:type="ByExpression">
                        <expression path="value" xsi:type="Property">
                           <source name="effective" xsi:type="IdentifierRef"/>
                        </expression>
                     </by>
                  </sort>
               </source>
            </element>
         </expression>
      </def>
      <def name="Dispensed Antidepressant Within 31 Days" context="Patient" accessLevel="Public">
         <expression xsi:type="Query">
            <source alias="Antidepressants">
               <expression dataType="fhir:MedicationDispense" codeProperty="medication" xsi:type="Retrieve">
                  <codes name="Antidepressant Medication" xsi:type="CodeRef"/>
               </expression>
            </source>
            <relationship alias="PositiveScreen" xsi:type="With">
               <expression name="First Positive Depression Screen During Follow-Up Period" xsi:type="ExpressionRef"/>
               <suchThat xsi:type="And">
                  <operand xsi:type="Equal">
                     <operand path="value" xsi:type="Property">
                        <source path="status" scope="Antidepressants" xsi:type="Property"/>
                     </operand>
                     <operand valueType="t:String" value="completed" xsi:type="Literal"/>
                  </operand>
                  <operand xsi:type="LessOrEqual">
                     <operand precision="Day" xsi:type="DurationBetween">
                        <operand path="value" xsi:type="Property">
                           <source path="whenHandedOver" scope="Antidepressants" xsi:type="Property"/>
                        </operand>
                        <operand path="value" xsi:type="Property">
                           <source path="effective" scope="PositiveScreen" xsi:type="Property"/>
                        </operand>
                     </operand>
                     <operand valueType="t:Integer" value="31" xsi:type="Literal"/>
                  </operand>
               </suchThat>
            </relationship>
            <return>
               <expression name="Antidepressants" xsi:type="AliasRef"/>
            </return>
         </expression>
      </def>
      <def name="Follow-Up Care Encounters" context="Patient" accessLevel="Public">
         <expression xsi:type="Distinct">
            <operand xsi:type="Query">
               <source alias="Enc">
                  <expression xsi:type="Union">
                     <operand xsi:type="Union">
                        <operand dataType="fhir:Encounter" codeProperty="type" xsi:type="Retrieve">
                           <codes name="Behavioral Health Encounter" xsi:type="ValueSetRef"/>
                        </operand>
                        <operand xsi:type="Query">
                           <source alias="E">
                              <expression dataType="fhir:Encounter" codeProperty="type" xsi:type="Retrieve">
                                 <codes name="ECDS Follow-Up Visit" xsi:type="ValueSetRef"/>
                              </expression>
                           </source>
                           <relationship alias="D" xsi:type="With">
                              <expression dataType="fhir:DiagnosticReport" codeProperty="code" xsi:type="Retrieve">
                                 <codes name="Major Depression and Dysthymia" xsi:type="ValueSetRef"/>
                              </expression>
                              <suchThat xsi:type="And">
                                 <operand xsi:type="And">
                                    <operand xsi:type="In">
                                       <operand path="value" xsi:type="Property">
                                          <source path="status" scope="E" xsi:type="Property"/>
                                       </operand>
                                       <operand xsi:type="List">
                                          <element valueType="t:String" value="planned" xsi:type="Literal"/>
                                          <element valueType="t:String" value="arrived" xsi:type="Literal"/>
                                          <element valueType="t:String" value="triaged" xsi:type="Literal"/>
                                          <element valueType="t:String" value="in-progress" xsi:type="Literal"/>
                                          <element valueType="t:String" value="onleave" xsi:type="Literal"/>
                                          <element valueType="t:String" value="finished" xsi:type="Literal"/>
                                       </operand>
                                    </operand>
                                    <operand xsi:type="In">
                                       <operand path="value" xsi:type="Property">
                                          <source path="status" scope="D" xsi:type="Property"/>
                                       </operand>
                                       <operand xsi:type="List">
                                          <element valueType="t:String" value="preliminary" xsi:type="Literal"/>
                                          <element valueType="t:String" value="final" xsi:type="Literal"/>
                                          <element valueType="t:String" value="amended" xsi:type="Literal"/>
                                          <element valueType="t:String" value="corrected" xsi:type="Literal"/>
                                          <element valueType="t:String" value="appended" xsi:type="Literal"/>
                                       </operand>
                                    </operand>
                                 </operand>
                                 <operand xsi:type="Overlaps">
                                    <operand name="PeriodToIntervalOfDT" xsi:type="FunctionRef">
                                       <operand asType="fhir:Period" xsi:type="As">
                                          <operand path="effective" scope="D" xsi:type="Property"/>
                                       </operand>
                                    </operand>
                                    <operand name="PeriodToIntervalOfDT" xsi:type="FunctionRef">
                                       <operand path="period" scope="E" xsi:type="Property"/>
                                    </operand>
                                 </operand>
                              </suchThat>
                           </relationship>
                        </operand>
                     </operand>
                     <operand dataType="fhir:Encounter" codeProperty="type" xsi:type="Retrieve">
                        <codes name="Depression Case Management Encounter" xsi:type="ValueSetRef"/>
                     </operand>
                  </expression>
               </source>
               <where xsi:type="In">
                  <operand path="value" xsi:type="Property">
                     <source path="status" scope="Enc" xsi:type="Property"/>
                  </operand>
                  <operand xsi:type="List">
                     <element valueType="t:String" value="planned" xsi:type="Literal"/>
                     <element valueType="t:String" value="arrived" xsi:type="Literal"/>
                     <element valueType="t:String" value="triaged" xsi:type="Literal"/>
                     <element valueType="t:String" value="in-progress" xsi:type="Literal"/>
                     <element valueType="t:String" value="onleave" xsi:type="Literal"/>
                     <element valueType="t:String" value="finished" xsi:type="Literal"/>
                  </operand>
               </where>
            </operand>
         </expression>
      </def>
      <def name="Follow-Up Encounter Within 31 Days" context="Patient" accessLevel="Public">
         <expression xsi:type="Query">
            <source alias="FollowUps">
               <expression name="Follow-Up Care Encounters" xsi:type="ExpressionRef"/>
            </source>
            <relationship alias="PositiveScreen" xsi:type="With">
               <expression name="First Positive Depression Screen During Follow-Up Period" xsi:type="ExpressionRef"/>
               <suchThat xsi:type="And">
                  <operand xsi:type="GreaterOrEqual">
                     <operand precision="Day" xsi:type="DurationBetween">
                        <operand xsi:type="Start">
                           <operand name="PeriodToIntervalOfDT" xsi:type="FunctionRef">
                              <operand path="period" scope="FollowUps" xsi:type="Property"/>
                           </operand>
                        </operand>
                        <operand path="value" xsi:type="Property">
                           <source path="effective" scope="PositiveScreen" xsi:type="Property"/>
                        </operand>
                     </operand>
                     <operand valueType="t:Integer" value="1" xsi:type="Literal"/>
                  </operand>
                  <operand xsi:type="LessOrEqual">
                     <operand precision="Day" xsi:type="DurationBetween">
                        <operand xsi:type="Start">
                           <operand name="PeriodToIntervalOfDT" xsi:type="FunctionRef">
                              <operand path="period" scope="FollowUps" xsi:type="Property"/>
                           </operand>
                        </operand>
                        <operand path="value" xsi:type="Property">
                           <source path="effective" scope="PositiveScreen" xsi:type="Property"/>
                        </operand>
                     </operand>
                     <operand valueType="t:Integer" value="31" xsi:type="Literal"/>
                  </operand>
               </suchThat>
            </relationship>
            <return>
               <expression name="FollowUps" xsi:type="AliasRef"/>
            </return>
         </expression>
      </def>
      <def name="Same Day Behavioral Encounter" context="Patient" accessLevel="Public">
         <expression xsi:type="Query">
            <source alias="BehavioralEncounter">
               <expression dataType="fhir:Encounter" codeProperty="type" xsi:type="Retrieve">
                  <codes name="Behavioral Health Encounter" xsi:type="ValueSetRef"/>
               </expression>
            </source>
            <relationship alias="PositiveScreen" xsi:type="With">
               <expression name="First Positive Depression Screen During Follow-Up Period" xsi:type="ExpressionRef"/>
               <suchThat xsi:type="And">
                  <operand xsi:type="In">
                     <operand path="value" xsi:type="Property">
                        <source path="status" scope="BehavioralEncounter" xsi:type="Property"/>
                     </operand>
                     <operand xsi:type="List">
                        <element valueType="t:String" value="planned" xsi:type="Literal"/>
                        <element valueType="t:String" value="arrived" xsi:type="Literal"/>
                        <element valueType="t:String" value="triaged" xsi:type="Literal"/>
                        <element valueType="t:String" value="in-progress" xsi:type="Literal"/>
                        <element valueType="t:String" value="onleave" xsi:type="Literal"/>
                        <element valueType="t:String" value="finished" xsi:type="Literal"/>
                     </operand>
                  </operand>
                  <operand precision="Day" xsi:type="SameAs">
                     <operand xsi:type="Start">
                        <operand name="PeriodToIntervalOfDT" xsi:type="FunctionRef">
                           <operand path="period" scope="BehavioralEncounter" xsi:type="Property"/>
                        </operand>
                     </operand>
